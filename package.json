{
  "devWatch": {
    "cmd": "dev",
    "runs": [
      {
        "type": "typescript",
        "browserify": false,
        "configFile": "src/tsconfig.json",
        "declarations": true
      }
    ]
  },
  "engines": {
    "node": "*"
  },
  "main": "./lib/index",
  "keywords": [
    "webdav-server",
    "webdav",
    "server",
    "virtual"
  ],
  "scripts": {
    "dev": "dev-watch package.json dev",
    "build": "dev-watch package.json prod",
    "test": "node test/root.js"
  },
  "name": "webdav-server",
  "version": "0.1.0",
  "author": "Adrien Castex <adrien.castex@gmail.com>",
  "license": "Unlicense",
  "description": "WebDAV Server",
  "dependencies": {
    "mime-types": "^2.1.15",
    "xmlbuilder": "^9.0.0"
  },
  "devDependencies": {
    "@types/mime-types": "^2.1.0",
    "@types/node": "^7.0.18",
    "@types/xmlbuilder": "0.0.30",
    "webdav-fs": "^1.0.0"
  }
}
