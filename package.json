{
  "devWatch": {
    "cmd": "dev",
    "runs": [
      {
        "type": "typescript",
        "browserify": false,
        "configFile": "src/tsconfig.json"
      }
    ]
  },
  "engines": {
    "node": "*"
  },
  "main": "./lib/index",
  "keywords": [
    "webdav",
    "server",
    "virtual"
